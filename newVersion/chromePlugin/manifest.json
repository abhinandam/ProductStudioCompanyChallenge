{
  "manifest_version": 2,

  "name": "Facepocket",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "img.png",
    "default_popup": "login.html"
  },

  "permissions": [
     "activeTab",
     "https://ajax.googleapis.com/"

  ]
}
